{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"types": [
			"vite/client"
		],
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"*.ts",
		"**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}